_G.distanceAlert = false
_G.survivorAlert = false
_G.genAlert = false

_G.alertKillerDist = 100
_G.alertSurvivorDist = 50
_G.alertGenDist = 20

_G.killerNotify = false
_G.survivorNotify = false
_G.genNotify = false

local killerNotified = false
local survivorNotified = {}
local genNotified = {}

